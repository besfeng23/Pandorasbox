{
  "firestore": {
    "database": "(default)",
    "location": "asia-southeast1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "apphosting": {
    "ignore": [
      "node_modules/",
      ".next/",
      "dummy/"
    ],
    "backends": [
      {
        "backendId": "studio",
        "rootDir": "backend",
        "webFramework": "NEXT_JS",
        "location": "us-central1"
      }
    ]
  },
  "storage": {
    "rules": "backend/storage.rules"
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": false
    },
    "singleProjectMode": true
  }
}
